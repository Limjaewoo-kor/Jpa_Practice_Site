    <style>
        body{width: 90%; height: 1000px; margin-left:5%; margin-right: 5%; background-color: burlywood;}
        header{margin-top: 40px;  width: 100%; height: 200px; border-bottom: black 1px solid }
        #header_photo{width: 170px; height: 170px;
            margin-left: 5px;
            border: 1px solid #AAA;
            display:block;
            float: left;}
        #header_a{display: inline; float: right; width: 200px;  }
        #header_a > a{margin-right: 3%;}
        #header_title{text-align: center; margin-top: 3%; margin-right: 15%; font-size: 50px; font-style: italic;}
        #header_ment{
            margin-left: 13%;
            display:inline;
            width: 300px;
            height: 25px;
            padding: 5px;
            background-color: white;
            animation-duration: 3s;
            animation-name: slidein;
            animation-iteration-count: 3;
            animation-direction: alternate;
            animation-timing-function: ease-in-out;}
        @keyframes slidein{
            from {margin-left: 50%; width: 400px; background-color: white;}
            to{ margin-left:10%; width: 200px; background-color: fuchsia;}
        }
        nav{ width: 20%; height: 700px; float:left;border-right: 1px solid black }
        nav > ul >li{margin-top: 20%; margin-bottom: 15%; font-size: 20px;}
        a{text-decoration: none; color: black;}
        a:hover{color: aquamarine;}
        a:active{color: rebeccapurple}
        #cal{width: 80%; height: 80%; float: right; }
        footer{width:100%; height: 100px;  float:left;  }
    </style>
